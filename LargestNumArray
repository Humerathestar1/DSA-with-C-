
#include <iostream>
#include <climits>
using namespace std;

int main() {
    int num[] = {25,67,89,-22,-78};
    int size = 5;
    int largest = INT_MIN;
    for(int i = 0;i<size;i++){
        if(num[i]>largest){
            largest = num[i];
        }
    }
    cout << "The largest no of an array is : "<<largest;

    return 0;
}
