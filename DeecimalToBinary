#include <iostream>
using namespace std;
int Binary(int DecNum){
    int ans = 0, pow = 1;
    while(DecNum > 0){
        int rem = DecNum%2;
        DecNum /= 2;
        ans += (rem*pow);
        pow*=10;
    }
    return ans;
}
int main() {
    
    int DecNum = 50;
    for(int i =0; i<=10; i++){
      cout<<Binary(i)<<endl;  
    }
    

    return 0;
}
